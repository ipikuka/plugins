{
  "name": "@ipikuka/plugins",
  "version": "0.0.1",
  "description": "The collection of useful remark, rehype, recma, mdx plugins and rehype handlers for markdown / MDX",
  "type": "module",
  "exports": "./dist/esm/index.js",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --build && type-coverage",
    "format": "npm run prettier && npm run lint",
    "prettier": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "test:file": "vitest test.spec.ts",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test && npm run format && npm run test-coverage",
    "test-coverage": "vitest run --coverage"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipikuka/plugins.git"
  },
  "keywords": [
    "unified",
    "remark",
    "mdast",
    "rehype",
    "hast",
    "recma",
    "esast",
    "markdown",
    "MDX",
    "remark plugins",
    "rehype plugins",
    "recma plugins",
    "rehype handlers"
  ],
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ipikuka/plugins#readme",
  "bugs": {
    "url": "https://github.com/ipikuka/plugins/issues"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@types/dedent": "^0.7.2",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitest/coverage-v8": "^1.4.0",
    "dedent": "^1.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "type-coverage": "^2.27.1",
    "typescript": "^5.4.3",
    "unified": "^11.0.4",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "hastscript": "^9.0.0",
    "mdast-util-to-hast": "^13.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pre-language": "^1.1.0",
    "rehype-prism-plus": "^2.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-definition-list": "^2.0.0",
    "remark-emoji": "^4.0.1",
    "remark-fix-guillemets": "^1.1.2",
    "remark-flexible-code-titles": "^1.2.0",
    "remark-flexible-containers": "^1.2.1",
    "remark-flexible-markers": "^1.2.1",
    "remark-flexible-paragraphs": "^1.2.0",
    "remark-flexible-toc": "^1.1.0",
    "remark-gemoji": "^8.0.0",
    "remark-gfm": "^4.0.0",
    "remark-ins": "^1.1.0",
    "remark-smartypants": "^2.1.0",
    "remark-supersub": "^1.0.0",
    "remark-textr": "^6.1.0",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreCatch": true,
    "strict": true
  },
  "sideEffects": false
}
